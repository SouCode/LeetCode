// Example 
// Constant time complexity example: Adding two numbers
function addNumbers(a, b) {
  return a + b;
}

const result = addNumbers(5, 3);
console.log(result); // Output: 8



// Example: Assuming sum is precomputed and stored

function getSumFromArray(sumArray, index) {
  if (index < 0 || index >= sumArray.length) {
    return null; // or any other default value indicating an error
  }
  return sumArray[index];
}

const precomputedSum = [1, 3, 6, 10, 15];
const index = 4;
const result = getSumFromArray(precomputedSum, index);

if (result === null) {
  console.log('Invalid index'); // Output: Invalid index
} else {
  console.log(result); // Output: 15
}

